{
    "manifest_version": 3,
    "name": "WhatsApp Emoji Suggester",
    "version": "1.0",
    "description": "Suggests emojis based on your WhatsApp messages.",
    "permissions": ["activeTab", "scripting"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }
  